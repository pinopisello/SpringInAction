<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://www.springframework.org/schema/context	http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="spitterServiceProxy"
		class="org.springframework.remoting.jaxws.JaxWsPortProxyFactoryBean"
		p:wsdlDocumentUrl="http://127.0.0.1:8080/SpitterService/ws/spitterService?wsdl"
		p:serviceName="SpitterService" 
		p:portName="SpitterServiceEndpointPort"
		p:serviceInterface="com.habuma.spitter.remoting.jaxws.server.SpitterServiceEndpointInterf"
		p:namespaceUri="http://pippo/SpitterService" /> 



<!-- 
p:serviceName 		=> <wsdl:service name="SpitterService"> in SpitterService.wsdl
p:portName    		=> <wsdl:port name="SpitterServiceEndpointPort"  in SpitterService.wsdl
p:serviceInterface  => interfaccia che descrive le operations del SpitterService
p:namespaceUri      => <wsdl:definitions name="SpitterService" targetNamespace="http://pippo/SpitterService"  in SpitterService.wsdl
  -->




</beans>